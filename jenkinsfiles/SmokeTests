pipeline {
    agent any

    stages {
        stage ('Build') {
          steps {
            sh 'mvn clean package'
          }
        }
        stage('Run tests') {
            steps {
                sh 'mvn clean verify -Denv=QA'
                git branch: 'HW_4.1_Selenide', url: 'https://github.com/sergei820/Selenide_Framework.git'
            }
        }
    }
}